{
  "info": {
    "name": "FOR THE KIDS API",
    "description": "Complete API collection for FOR THE KIDS platform\n\nMission: 60% of revenue to Verified Pediatric Charities\nGospel V1.3 - Ethics Override\n\nBase URL: https://api.aidoesitall.website",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json",
    "version": "1.3.0"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "value",
        "value": "{{API_KEY}}",
        "type": "string"
      },
      {
        "key": "key",
        "value": "X-API-Key",
        "type": "string"
      }
    ]
  },
  "variable": [
    {
      "key": "BASE_URL",
      "value": "https://api.aidoesitall.website",
      "type": "string"
    },
    {
      "key": "API_KEY",
      "value": "your_api_key_here",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Public Endpoints",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/health",
              "host": ["{{BASE_URL}}"],
              "path": ["health"]
            },
            "description": "Check API service health status"
          }
        },
        {
          "name": "Gospel Revenue Split",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/gospel",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "gospel"]
            },
            "description": "Get immutable 60/30/10 Gospel split configuration"
          }
        },
        {
          "name": "Transparency Statistics",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/transparency/stats",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "transparency", "stats"]
            },
            "description": "Get lifetime platform statistics and revenue distribution"
          }
        },
        {
          "name": "Current Month Report",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/transparency/current-month",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "transparency", "current-month"]
            },
            "description": "Get real-time revenue data for current month"
          }
        },
        {
          "name": "Monthly Reports",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/transparency/monthly-reports",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "transparency", "monthly-reports"]
            },
            "description": "Get all historical monthly revenue reports"
          }
        },
        {
          "name": "Kickstarter Campaign Info",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/kickstarter/info",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "kickstarter", "info"]
            },
            "description": "Get live Kickstarter campaign statistics"
          }
        },
        {
          "name": "Kickstarter Stats",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/kickstarter/stats",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "kickstarter", "stats"]
            },
            "description": "Get detailed campaign statistics"
          }
        },
        {
          "name": "Kickstarter Backers List",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/kickstarter/backers",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "kickstarter", "backers"]
            },
            "description": "Get list of campaign backers (anonymized)"
          }
